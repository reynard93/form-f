{
  "name": "form-facilitator",
  "version": "0.5.0",
  "main": "./dist/system.umd.js",
  "module": "./dist/system.es.js",
  "exports": {
    ".": {
      "import": "./dist/system.es.js",
      "require": "./dist/system.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite --config vite.config.ts",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test": "vitest",
    "test:dev": "vitest --watch-all --coverage false",
    "build-only": "vite build --config vite.config.ts",
    "type-check": "vue-tsc --noEmit --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@momwins/mom-design-system-v3": "3.0.18",
    "pinia": "^2.0.32",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@momwins/wins-form-schemas": "^1.20.0",
    "@pinia/testing": "0.0.16",
    "@rushstack/eslint-patch": "^1.2.0",
    "@testing-library/vue": "^7.0.0",
    "@types/jsdom": "^21.1.0",
    "@types/node": "^18.14.2",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.3.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.34.0",
    "eslint-plugin-vitest": "^0.2.6",
    "eslint-plugin-vue": "^9.9.0",
    "jsdom": "^21.1.0",
    "miragejs": "^0.1.47",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "source-map-support": "^0.5.21",
    "ts-node": "10.9.1",
    "typescript": "~5.1.3",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.1.4",
    "vitest": "^0.29.1",
    "vue-tsc": "^1.2.0",
    "yalc": "^1.0.0-pre.53"
  }
}
